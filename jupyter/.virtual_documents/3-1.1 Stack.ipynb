def toList(orig):
    temp = Stack()
    stackList = []
    
    while not orig.is_empty():
        item = orig.pop()
        stackList.append(item)
        temp.push(item)
    while not temp.is_empty():
        orig.push(temp.pop())
    
    return stackList

